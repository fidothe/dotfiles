#!/bin/bash

source "$(dirname $0)/../sh-common/brew-helpers.sh"
#Â leave this homebrew / mac only for now
if brew_up; then
  gopath="$(brew_path --prefix go)/libexec/misc/vim"
  vimpath=$HOME/.vim/pathogen_style
  if [ -d $gopath ]; then
    mkdir -p $vimpath
    ln -sf $gopath $vimpath/go
  fi
fi
